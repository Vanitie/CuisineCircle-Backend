        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ccb.mapper.PreferenceMapper">
    <select id="selectByUserId" parameterType="java.lang.Integer" resultType="com.ccb.model.pojo.Preference">
