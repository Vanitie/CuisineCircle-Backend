        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ccb.mapper.ReadHistoryMapper">
    <select id="selectByUserId" resultType="com.ccb.model.pojo.ReadHistory">
        SELECT * FROM read_history WHERE user_id = #{userId}
